extends layout

block content
    h2 Login
    p Please login below.
    form(method='post', action='/users/login',enctype="multipart/form-data")
        .form-group
            label Username
            input.form-control(type='text',name='username')
        .form-group
            label Password
            input.form-control(type='password',name='password')
        input.btn.btn-default(type='submit',name='submit',value='Login')